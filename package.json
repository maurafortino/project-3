  "devDependencies": {
    "concurrently": "^5.0.2",
    "nodemon": "^2.0.2"
  },
  "bugs": {
    "url": "https://github.com/maurafortino/project-3/issues"
  },
  "homepage": "https://github.com/maurafortino/project-3#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^3.0.7",
    "create-react-app": "^3.3.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.8.7",
    "passport-google-oauth": "^2.0.0"
  }
